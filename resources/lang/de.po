msgstr "Telefonnummer inkl. Landesvorwahl, z.B. \"+49 30 123456\""

msgid "Fax number including country prefix, e.g. \"+49 30 654321\""
msgstr "Telefaxnummer inkl. Landesvorwahl, z.B. \"+49 30 654321\""

